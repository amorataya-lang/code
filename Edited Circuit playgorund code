import board
import digitalio
import time

led = digitalio.DigitalInOut(board.LED)
led.direction = digitalio.Direction.OUTPUT

print(dir(led))
print(led.value)
print(led.direction)
print()

print(dir(time))
print()

x = time.localtime()
print(x)

while True:
    led.value = True    # LED ON
    time.sleep(0.5)
    led.value = False   # LED OFF
    time.sleep(0.5)
